	package TRABAJO;
/**
 * 
 *@author Alexandro 
 *
 */
public class Producto {
static String Nombre;
static Float Precio;
static Double preciofinal;
static int Cantidad;


public Producto(String Nombre, Float Precio, int Cantidad) {
	this.Nombre=Nombre;
	this.Precio=Precio;
	this.Cantidad=Cantidad;
}

public String getNombre() {
return Nombre;
}

public Float getPrecio() {
return Precio;
}

}
